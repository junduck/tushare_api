{
  "name": "tushare_stk_week_month_adj",
  "description": "周/月线复权行情(每日更新)。股票周/月线行情(复权--每日更新)限量：单次最大6000,可使用交易日期循环提取，总量不限制积分：用户需要至少2000积分才可以调取，具体请参阅积分获取办法。",
  "inputSchema": {
    "type": "object",
    "properties": {
      "ts_code": {
        "type": "string",
        "description": "股票代码"
      },
      "trade_date": {
        "type": "string",
        "description": "交易日期（每周五或者月末日期）",
        "pattern": "^\\d{8}$"
      },
      "end_date": {
        "type": "string",
        "description": "计算截至日期",
        "pattern": "^\\d{8}$"
      },
      "freq": {
        "type": "string",
        "description": "频率(周week,月month)"
      },
      "open": {
        "type": "number",
        "description": "(周/月)开盘价"
      },
      "high": {
        "type": "number",
        "description": "(周/月)最高价"
      },
      "low": {
        "type": "number",
        "description": "(周/月)最低价"
      },
      "close": {
        "type": "number",
        "description": "(周/月)收盘价"
      },
      "pre_close": {
        "type": "number",
        "description": "上一(周/月)收盘价【除权价，前复权】"
      },
      "open_qfq": {
        "type": "number",
        "description": "前复权(周/月)开盘价"
      },
      "high_qfq": {
        "type": "number",
        "description": "前复权(周/月)最高价"
      },
      "low_qfq": {
        "type": "number",
        "description": "前复权(周/月)最低价"
      },
      "close_qfq": {
        "type": "number",
        "description": "前复权(周/月)收盘价"
      },
      "open_hfq": {
        "type": "number",
        "description": "后复权(周/月)开盘价"
      },
      "high_hfq": {
        "type": "number",
        "description": "后复权(周/月)最高价"
      },
      "low_hfq": {
        "type": "number",
        "description": "后复权(周/月)最低价"
      },
      "close_hfq": {
        "type": "number",
        "description": "后复权(周/月)收盘价"
      },
      "vol": {
        "type": "number",
        "description": "(周/月)成交量"
      },
      "amount": {
        "type": "number",
        "description": "(周/月)成交额"
      },
      "change": {
        "type": "number",
        "description": "(周/月)涨跌额"
      },
      "pct_chg": {
        "type": "number",
        "description": "(周/月)涨跌幅 【基于除权后的昨收计算的涨跌幅：（今收-除权昨收）/除权昨收 】"
      }
    },
    "required": [
      "ts_code",
      "trade_date",
      "end_date",
      "freq",
      "open",
      "high",
      "low",
      "close",
      "pre_close",
      "open_qfq",
      "high_qfq",
      "low_qfq",
      "close_qfq",
      "open_hfq",
      "high_hfq",
      "low_hfq",
      "close_hfq",
      "vol",
      "amount",
      "change",
      "pct_chg"
    ]
  }
}