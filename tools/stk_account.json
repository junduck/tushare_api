{
  "name": "tushare_stk_account",
  "description": "股票开户数据（停）。获取股票账户开户数据，统计周期为一周积分：600积分可调取，具体请参阅积分获取办法。返回字段包括：date(Y), weekly_new(Y), total(Y), weekly_hold(Y), weekly_trade(Y)。",
  "inputSchema": {
    "type": "object",
    "properties": {
      "date": {
        "type": "string",
        "description": "日期",
        "pattern": "^\\d{8}$"
      },
      "start_date": {
        "type": "string",
        "description": "开始日期",
        "pattern": "^\\d{8}$"
      },
      "end_date": {
        "type": "string",
        "description": "结束日期",
        "pattern": "^\\d{8}$"
      }
    },
    "required": []
  }
}