{
  "name": "tushare_bond_blk",
  "description": "大宗交易。获取沪深交易所债券大宗交易数据，可以通过数据工具调试和查看数据。。返回字段包括：trade_date(Y), ts_code(Y), name(Y), price(Y), vol(Y), amount(Y)。",
  "inputSchema": {
    "type": "object",
    "properties": {
      "ts_code": {
        "type": "string",
        "description": "债券代码"
      },
      "trade_date": {
        "type": "string",
        "description": "交易日期（YYYYMMDD格式，下同）",
        "pattern": "^\\d{8}$"
      },
      "start_date": {
        "type": "string",
        "description": "开始日期",
        "pattern": "^\\d{8}$"
      },
      "end_date": {
        "type": "string",
        "description": "结束日期",
        "pattern": "^\\d{8}$"
      }
    },
    "required": []
  }
}