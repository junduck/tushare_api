{
  "api_name": "cb_call",
  "title": "可转债赎回信息",
  "description": "获取可转债到期赎回、强制赎回等信息。数据来源于公开披露渠道，供个人和机构研究使用，请不要用于数据商业目的。限量：单次最大2000条数据，可以根据日期循环提取，本接口需5000积分。",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "转债代码，支持多值输入"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "N",
      "description": "公告日期(YYYYMMDD格式，下同)"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "公告开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "公告结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "转债代码"
    },
    {
      "name": "call_type",
      "type": "str",
      "required": "Y",
      "description": "赎回类型：到赎、强赎"
    },
    {
      "name": "is_call",
      "type": "str",
      "required": "Y",
      "description": "是否赎回：已满足强赎条件、公告提示强赎、公告实施强赎、公告到期赎回、公告不强赎"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "Y",
      "description": "公告/提示日期"
    },
    {
      "name": "call_date",
      "type": "str",
      "required": "Y",
      "description": "赎回日期"
    },
    {
      "name": "call_price",
      "type": "float",
      "required": "Y",
      "description": "赎回价格(含税，元/张)"
    },
    {
      "name": "call_price_tax",
      "type": "float",
      "required": "Y",
      "description": "赎回价格(扣税，元/张)"
    },
    {
      "name": "call_vol",
      "type": "float",
      "required": "Y",
      "description": "赎回债券数量(张)"
    },
    {
      "name": "call_amount",
      "type": "float",
      "required": "Y",
      "description": "赎回金额(万元)"
    },
    {
      "name": "payment_date",
      "type": "str",
      "required": "Y",
      "description": "行权后款项到账日"
    },
    {
      "name": "call_reg_date",
      "type": "str",
      "required": "Y",
      "description": "赎回登记日"
    }
  ]
}