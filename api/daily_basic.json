{
  "api_name": "daily_basic",
  "title": "每日指标",
  "description": "获取全部股票每日重要的基本面指标，可用于选股分析、报表展示等。单次请求最大返回6000条数据，可按日线循环提取全部历史。积分：至少2000积分才可以调取，5000积分无总量限制，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码（二选一）"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期 （二选一）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期(YYYYMMDD)"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期(YYYYMMDD)"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "TS股票代码",
      "description": ""
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "交易日期",
      "description": ""
    },
    {
      "name": "close",
      "type": "float",
      "required": "当日收盘价",
      "description": ""
    },
    {
      "name": "turnover_rate",
      "type": "float",
      "required": "换手率（%）",
      "description": ""
    },
    {
      "name": "turnover_rate_f",
      "type": "float",
      "required": "换手率（自由流通股）",
      "description": ""
    },
    {
      "name": "volume_ratio",
      "type": "float",
      "required": "量比",
      "description": ""
    },
    {
      "name": "pe",
      "type": "float",
      "required": "市盈率（总市值/净利润， 亏损的PE为空）",
      "description": ""
    },
    {
      "name": "pe_ttm",
      "type": "float",
      "required": "市盈率（TTM，亏损的PE为空）",
      "description": ""
    },
    {
      "name": "pb",
      "type": "float",
      "required": "市净率（总市值/净资产）",
      "description": ""
    },
    {
      "name": "ps",
      "type": "float",
      "required": "市销率",
      "description": ""
    },
    {
      "name": "ps_ttm",
      "type": "float",
      "required": "市销率（TTM）",
      "description": ""
    },
    {
      "name": "dv_ratio",
      "type": "float",
      "required": "股息率 （%）",
      "description": ""
    },
    {
      "name": "dv_ttm",
      "type": "float",
      "required": "股息率（TTM）（%）",
      "description": ""
    },
    {
      "name": "total_share",
      "type": "float",
      "required": "总股本 （万股）",
      "description": ""
    },
    {
      "name": "float_share",
      "type": "float",
      "required": "流通股本 （万股）",
      "description": ""
    },
    {
      "name": "free_share",
      "type": "float",
      "required": "自由流通股本 （万）",
      "description": ""
    },
    {
      "name": "total_mv",
      "type": "float",
      "required": "总市值 （万元）",
      "description": ""
    },
    {
      "name": "circ_mv",
      "type": "float",
      "required": "流通市值（万元）",
      "description": ""
    }
  ]
}