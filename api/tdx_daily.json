{
  "api_name": "tdx_daily",
  "title": "通达信板块行情",
  "description": "获取通达信各板块行情，包括成交和估值等数据限量：单次提取最大3000条数据，可根据板块代码和日期参数循环提取权限：用户积累6000积分可调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "板块代码：xxxxxx.TDX"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期，格式YYYYMMDD,下同"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "板块代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘点位"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘点位"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高点位"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低点位"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "昨日收盘点"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "涨跌点位"
    },
    {
      "name": "pct_change",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅%"
    },
    {
