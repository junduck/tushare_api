{
  "api_name": "kpl_concept",
  "title": "题材数据（开盘啦）",
  "description": "获取开盘啦概念题材列表，每天盘后更新限量：单次最大5000条，可根据日期循环获取历史数据积分：5000积分可提取数据，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式）"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "题材代码（xxxxxx.KP格式）"
    },
    {
      "name": "name",
      "type": "str",
      "required": "N",
      "description": "题材名称"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "题材代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "题材名称"
    },
    {
      "name": "z_t_num",
      "type": "None",
      "required": "Y",
      "description": "涨停数量"
    },
    {
      "name": "up_num",
      "type": "str",
      "required": "Y",
      "description": "排名上升位数"
    }
  ]
}