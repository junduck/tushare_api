{
  "api_name": "forecast",
  "title": "业绩预告",
  "description": "获取业绩预告数据权限：用户需要至少2000积分才可以调取，具体请参阅积分获取办法提示：当前接口只能按单只股票获取其历史数据，如果需要获取某一季度全部上市公司数据，请使用forecast_vip接口（参数一致），需积攒5000积分。",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "股票代码(二选一)"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "N",
      "description": "公告日期 (二选一)"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "公告开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "公告结束日期"
    },
    {
      "name": "period",
      "type": "str",
      "required": "N",
      "description": "报告期(每个季度最后一天的日期，比如20171231表示年报，20170630半年报，20170930三季报)"
    },
    {
      "name": "type",
      "type": "str",
      "required": "N",
      "description": "预告类型(预增/预减/扭亏/首亏/续亏/续盈/略增/略减)"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "TS股票代码",
      "description": ""
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "公告日期",
      "description": ""
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "报告期",
      "description": ""
    },
    {
      "name": "type",
      "type": "str",
      "required": "业绩预告类型(预增/预减/扭亏/首亏/续亏/续盈/略增/略减)",
      "description": ""
    },
    {
      "name": "p_change_min",
      "type": "float",
      "required": "预告净利润变动幅度下限（%）",
      "description": ""
    },
    {
      "name": "p_change_max",
      "type": "float",
      "required": "预告净利润变动幅度上限（%）",
      "description": ""
    },
    {
      "name": "net_profit_min",
      "type": "float",
      "required": "预告净利润下限（万元）",
      "description": ""
    },
    {
      "name": "net_profit_max",
      "type": "float",
      "required": "预告净利润上限（万元）",
      "description": ""
    },
    {
      "name": "last_parent_net",
      "type": "float",
      "required": "上年同期归属母公司净利润",
      "description": ""
    },
    {
      "name": "first_ann_date",
      "type": "str",
      "required": "首次公告日",
      "description": ""
    },
    {
      "name": "summary",
      "type": "str",
      "required": "业绩预告摘要",
      "description": ""
    },
    {
      "name": "change_reason",
      "type": "str",
      "required": "业绩变动原因",
      "description": ""
    }
  ]
}