{
  "api_name": "daily_info",
  "title": "沪深市场每日交易统计",
  "description": "获取交易所股票交易统计，包括各板块明细限量：单次最大4000，可循环获取，总量不限制权限：用户积600积分可调取， 频次有限制，积分越高每分钟调取频次越高，5000积分以上频次相对较高，积分获取方法请参阅积分获取办法",
  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式，下同）"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "板块代码（请参阅下方列表）"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "股票市场（SH上交所 SZ深交所）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "fields",
      "type": "str",
      "required": "N",
      "description": "指定提取字段"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
