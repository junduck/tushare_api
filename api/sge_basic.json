{
  "api_name": "sge_basic",
  "title": "上海黄金基础信息",
  "description": "获取上海黄金交易所现货合约基础信息限量：单次最大100条，当前现货合约数不足20个，可以一次提取全部，不需要循环提取积分：用户积5000积分可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "合约代码 （支持多个，逗号分隔，不输入为获取全部）"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "品种代码"
    },
    {
      "name": "ts_name",
      "type": "str",
      "required": "Y",
      "description": "品种名称"
    },
    {
      "name": "trade_type",
      "type": "str",
      "required": "Y",
      "description": "交易类型"
    },
    {
      "name": "t_unit",
      "type": "float",
      "required": "Y",
      "description": "交易单位(克/手)"
    },
    {
      "name": "p_unit",
      "type": "float",
      "required": "Y",
      "description": "报价单位"
    },
    {
      "name": "min_change",
      "type": "float",
      "required": "Y",
      "description": "最小变动价位"
    },
    {
      "name": "price_limit",
      "type": "float",
      "required": "Y",
      "description": "每日价格最大波动限制"
    },
    {
      "name": "min_vol",
      "type": "int",
      "required": "Y",
      "description": "最小单笔报价量(手)"
    },
    {
      "name": "max_vol",
      "type": "int",
      "required": "Y",
      "description": "最大单笔报价量(手)"
    },
    {
      "name": "trade_mode",
      "type": "str",
      "required": "Y",
      "description": "交易期限"
    },
    {
      "name": "margin_rate",
      "type": "float",
      "required": "Y",
      "description": "保证金比例"
    },
    {
      "name": "liq_rate",
      "type": "float",
      "required": "Y",
      "description": "违约金比例(%)"
    },
    {
      "name": "trade_time",
      "type": "str",
      "required": "Y",
      "description": "交易时间"
    },
    {
      "name": "list_date",
      "type": "str",
      "required": "Y",
      "description": "上市日期"
    }
  ]
}