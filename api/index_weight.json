    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（格式YYYYMMDD，下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "None",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "index_code",
      "type": "str",
      "required": "指数代码",
      "description": ""
    },
    {
      "name": "con_code",
      "type": "str",
      "required": "成分代码",
      "description": ""
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "交易日期",
      "description": ""
    },
    {
      "name": "weight",
      "type": "float",
      "required": "权重",
      "description": ""
    }
  ]
}