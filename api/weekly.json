{
  "api_name": "weekly",
  "title": "周线行情",
  "description": "获取A股周线行情，本接口每周最后一个交易日更新，如需要使用每天更新的周线数据，请使用日度更新的周线行情接口。限量：单次最大6000行，可使用交易日期循环提取，总量不限制积分：用户需要至少2000积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS代码 （ts_code,trade_date两个参数任选一）"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期 （每周最后一个交易日期，YYYYMMDD格式）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "周收盘价"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "周开盘价"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "周最高价"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "周最低价"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "上一周收盘价"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "周涨跌额"
    },
    {
      "name": "pct_chg",
      "type": "float",
      "required": "Y",
      "description": "周涨跌 （未复权，未100，如果是复权请用 通用行情接口，如需%单位请100 ）"
    },
    {
      "name": "vol",
      "type": "float",
      "required": "Y",
      "description": "周成交量"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "周成交额"
    }
  ]
}