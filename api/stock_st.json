    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（格式：YYYYMMDD下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始时间"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束时间"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "type",
      "type": "str",
      "required": "Y",
      "description": "类型"
    },
    {
      "name": "type_name",
      "type": "str",
      "required": "Y",
      "description": "类型名称"
    }
  ]
}