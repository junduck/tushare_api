{
  "api_name": "bo_monthly",
  "title": "电影月度票房",
  "description": "获取电影月度票房数据数据更新：本月更新上一月数据数据历史： 数据从2008年1月1日开始，超过10年历史数据。数据权限：用户需要至少500积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期（每月1号，格式YYYYMMDD）"
    }
  ],
  "output_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "影片名称"
    },
    {
      "name": "list_date",
      "type": "str",
      "required": "Y",
      "description": "上映日期"
    },
    {
      "name": "avg_price",
      "type": "float",
      "required": "Y",
      "description": "平均票价"
    },
    {
      "name": "month_amount",
      "type": "float",
      "required": "Y",
      "description": "当月票房（万）"
    },
    {
      "name": "list_day",
      "type": "int",
      "required": "Y",
      "description": "月内天数"
    },
    {
      "name": "p_pc",
      "type": "int",
      "required": "Y",
      "description": "场均人次"
    },
    {
      "name": "wom_index",
      "type": "float",
      "required": "Y",
      "description": "口碑指数"
    },
    {
      "name": "m_ratio",
      "type": "float",
      "required": "Y",
      "description": "月度占比（%）"
    },
    {
      "name": "rank",
      "type": "int",
      "required": "Y",
      "description": "排名"
    }
  ]
}