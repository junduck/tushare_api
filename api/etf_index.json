    },
    {
      "name": "pub_date",
      "type": "str",
      "required": "N",
      "description": "发布日期（格式：YYYYMMDD）"
    },
    {
      "name": "base_date",
      "type": "str",
      "required": "N",
      "description": "指数基期（格式：YYYYMMDD）"
