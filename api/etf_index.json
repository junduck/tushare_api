    {
      "name": "pub_party_name",
      "type": "str",
      "required": "Y",
      "description": "指数发布机构"
    },
    {
      "name": "pub_date",
      "type": "str",
      "required": "Y",
      "description": "指数发布日期"
    },
    {
      "name": "base_date",
      "type": "str",
      "required": "Y",
      "description": "指数基日"
    },
    {
