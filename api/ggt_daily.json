{
  "api_name": "ggt_daily",
  "title": "港股通每日成交统计",
  "description": "获取港股通每日成交信息，数据从2014年开始限量：单次最大1000，总量数据不限制积分：用户积2000积分可调取，5000积分以上频次相对较高，请自行提高积分，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期 （格式YYYYMMDD，下同。支持单日和多日输入）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "buy_amount",
      "type": "float",
      "required": "Y",
      "description": "买入成交金额（亿元）"
    },
    {
      "name": "buy_volume",
      "type": "float",
      "required": "Y",
      "description": "买入成交笔数（万笔）"
    },
    {
      "name": "sell_amount",
      "type": "float",
      "required": "Y",
      "description": "卖出成交金额（亿元）"
    },
    {
      "name": "sell_volume",
      "type": "float",
      "required": "Y",
      "description": "卖出成交笔数（万笔）"
    }
  ]
}