{
  "api_name": "bo_daily",
  "title": "电影日度票房",
  "description": "获取电影日度票房数据更新：当日更新上一日数据数据历史： 数据从2018年9月开始，更多历史数据正在补充数据权限：用户需要至少500积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期 （格式YYYYMMDD）"
    }
  ],
  "output_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "影片名称"
    },
    {
      "name": "avg_price",
      "type": "float",
      "required": "Y",
      "description": "平均票价"
    },
    {
      "name": "day_amount",
      "type": "float",
      "required": "Y",
      "description": "当日票房（万）"
    },
    {
      "name": "total",
      "type": "float",
      "required": "Y",
      "description": "累计票房（万）"
    },
    {
      "name": "list_day",
      "type": "int",
      "required": "Y",
      "description": "上映天数"
    },
    {
      "name": "p_pc",
      "type": "int",
      "required": "Y",
      "description": "场均人次"
    },
    {
      "name": "wom_index",
      "type": "float",
      "required": "Y",
      "description": "口碑指数"
    },
    {
      "name": "up_ratio",
      "type": "float",
      "required": "Y",
      "description": "环比变化 （%）"
    },
    {
      "name": "rank",
      "type": "int",
      "required": "Y",
      "description": "排名"
    }
  ]
}