{
  "api_name": "ci_daily",
  "title": "中信行业指数日行情",
  "description": "获取中信行业指数日线行情限量：单次最大4000条，可循环提取积分：5000积分可调取，可通过指数代码和日期参数循环获取所有数据",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "行业代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式，下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "指数代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘点位"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低点位"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高点位"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘点位"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "昨日收盘点位"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "涨跌点位"
    },
    {
      "name": "pct_change",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅"
    },
    {
      "name": "vol",
      "type": "float",
      "required": "Y",
      "description": "成交量（万股）"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交额（万元）"
    }
  ]
}