{
  "api_name": "bo_weekly",
  "title": "电影周度票房",
  "description": "获取周度票房数据数据更新：本周更新上一周数据数据历史： 数据从2008年第一周开始，超过10年历史数据。数据权限：用户需要至少500积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期（每周一日期，格式YYYYMMDD）"
    }
  ],
  "output_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "日期"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "影片名称"
    },
    {
      "name": "avg_price",
      "type": "float",
      "required": "Y",
      "description": "平均票价"
    },
    {
      "name": "week_amount",
      "type": "float",
      "required": "Y",
      "description": "当周票房（万）"
    },
    {
      "name": "total",
      "type": "float",
      "required": "Y",
      "description": "累计票房（万）"
    },
    {
      "name": "list_day",
      "type": "int",
      "required": "Y",
      "description": "上映天数"
    },
    {
      "name": "p_pc",
      "type": "int",
      "required": "Y",
      "description": "场均人次"
    },
    {
      "name": "wom_index",
      "type": "float",
      "required": "Y",
      "description": "口碑指数"
    },
    {
      "name": "up_ratio",
      "type": "float",
      "required": "Y",
      "description": "环比变化 （%）"
    },
    {
      "name": "rank",
      "type": "int",
      "required": "Y",
      "description": "排名"
    }
  ]
}