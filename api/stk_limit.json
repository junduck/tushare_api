      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "TS股票代码"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "N",
