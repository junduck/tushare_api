{
  "api_name": "index_dailybasic",
  "title": "大盘指数每日指标",
  "description": "目前只提供上证综指，深证成指，上证50，中证500，中小板指，创业板指的每日指标数据数据来源：Tushare社区统计计算数据历史：从2004年1月开始提供数据权限：用户需要至少400积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期 （格式：YYYYMMDD，比如20181018，下同）"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS代码"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "TS代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "total_mv",
      "type": "float",
      "required": "Y",
      "description": "当日总市值（元）"
    },
    {
      "name": "float_mv",
      "type": "float",
      "required": "Y",
      "description": "当日流通市值（元）"
    },
    {
      "name": "total_share",
      "type": "float",
      "required": "Y",
      "description": "当日总股本（股）"
    },
    {
      "name": "float_share",
      "type": "float",
      "required": "Y",
      "description": "当日流通股本（股）"
    },
    {
      "name": "free_share",
      "type": "float",
      "required": "Y",
      "description": "当日自由流通股本（股）"
    },
    {
      "name": "turnover_rate",
      "type": "float",
      "required": "Y",
      "description": "换手率"
    },
    {
      "name": "turnover_rate_f",
      "type": "float",
      "required": "Y",
