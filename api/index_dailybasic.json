    {
      "name": "pe",
      "type": "float",
      "required": "Y",
      "description": "市盈率"
    },
    {
      "name": "pe_ttm",
      "type": "float",
      "required": "Y",
      "description": "市盈率TTM"
    },
    {
      "name": "pb",
      "type": "float",
      "required": "Y",
      "description": "市净率"
    }
  ]
}