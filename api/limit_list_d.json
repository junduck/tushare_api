{
  "api_name": "limit_list_d",
  "title": "涨跌停和炸板数据",
  "description": "获取A股每日涨跌停、炸板数据情况，数据从2020年开始（不提供ST股票的统计）限量：单次最大可以获取2500条数据，可通过日期或者股票循环提取积分：5000积分每分钟可以请求200次每天总量1万次，8000积分以上每分钟500次每天总量不限制，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "股票代码"
    },
    {
      "name": "limit_type",
      "type": "str",
      "required": "N",
      "description": "涨跌停类型（U涨停D跌停Z炸板）"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易所（SH上交所SZ深交所BJ北交所）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "industry",
      "type": "str",
      "required": "Y",
      "description": "所属行业"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘价"
    },
    {
      "name": "pct_chg",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交额"
    },
    {
      "name": "limit_amount",
      "type": "float",
      "required": "Y",
      "description": "板上成交金额(成交价格为该股票跌停价的所有成交额的总和，涨停无此数据)"
    },
    {
      "name": "float_mv",
      "type": "float",
      "required": "Y",
      "description": "流通市值"
    },
    {
      "name": "total_mv",
      "type": "float",
      "required": "Y",
      "description": "总市值"
    },
    {
      "name": "turnover_ratio",
      "type": "float",
      "required": "Y",
      "description": "换手率"
    },
    {
      "name": "fd_amount",
      "type": "float",
      "required": "Y",
      "description": "封单金额（以涨停价买入挂单的资金总量）"
    },
    {
      "name": "first_time",
      "type": "str",
      "required": "Y",
      "description": "首次封板时间（跌停无此数据）"
    },
    {
      "name": "last_time",
      "type": "str",
      "required": "Y",
      "description": "最后封板时间"
    },
    {
      "name": "open_times",
      "type": "int",
      "required": "Y",
      "description": "炸板次数(跌停为开板次数)"
    },
    {
      "name": "up_stat",
      "type": "str",
      "required": "Y",
      "description": "涨停统计（N/T T天有N次涨停）"
    },
    {
      "name": "limit_times",
      "type": "int",
      "required": "Y",
      "description": "连板数（个股连续封板数量）"
    },
    {
      "name": "limit",
      "type": "str",
      "required": "Y",
      "description": "D跌停U涨停Z炸板"
    }
  ]
}