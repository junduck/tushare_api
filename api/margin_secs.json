{
  "api_name": "margin_secs",
  "title": "融资融券标的（盘前）",
  "description": "获取沪深京三大交易所融资融券标的（包括ETF），每天盘前更新限量：单次最大6000行数据，可根据股票代码、交易日期、交易所代码循环提取积分：2000积分可调取，5000积分无总量限制，积分越高权限越大，具体参考权限说明",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "标的代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易所（SSE上交所 SZSE深交所 BSE北交所）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "标的代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "标的名称"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "Y",
      "description": "交易所"
    }
  ]
}