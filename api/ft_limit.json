{
  "api_name": "ft_limit",
  "title": "期货合约涨跌停价格",
  "description": "获取所有期货合约每天的涨跌停价格及最低保证金率，数据开始于2005年。限量：单次最大获取4000行数据，可以通过日期、合约代码等参数循环获取所有历史积分：用户积5000积分可调取，积分获取方法具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "合约代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（格式：YYYYMMDD）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "cont",
      "type": "str",
      "required": "N",
      "description": "合约代码（例如：cont='CU')"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易所代码 （例如：exchange='DCE')"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "TS股票代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "合约名称"
    },
    {
      "name": "up_limit",
      "type": "float",
      "required": "Y",
      "description": "涨停价"
    },
    {
      "name": "down_limit",
      "type": "float",
      "required": "Y",
      "description": "跌停价"
    },
    {
      "name": "m_ratio",
      "type": "float",
      "required": "Y",
      "description": "最低交易保证金率（%）"
    },
    {
      "name": "cont",
      "type": "str",
      "required": "Y",
      "description": "合约代码"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "Y",
      "description": "交易所代码"
    }
  ]
}