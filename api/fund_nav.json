  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS基金代码 （二选一）"
    },
    {
      "name": "nav_date",
      "type": "str",
      "required": "N",
      "description": "净值日期 （二选一）"
    },
    {
      "name": "market",
      "type": "str",
      "required": "N",
      "description": "E场内 O场外"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "净值开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "净值结束日期"
    }
