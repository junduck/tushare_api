    },
    {
      "name": "con_code",
      "type": "str",
      "required": "N",
      "description": "成分股票代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式）"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "概念代码"
    },
    {
      "name": "con_code",
      "type": "str",
      "required": "Y",
      "description": "成分代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "成分股名称"
    }
  ]
}