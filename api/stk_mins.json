{
  "api_name": "stk_mins",
  "title": "ETF分钟行情",
  "description": "获取ETF分钟数据，支持1min/5min/15min/30min/60min行情，提供Python SDK和 http Restful API两种方式限量：单次最大8000行数据，可以通过股票代码和时间循环获取，本接口可以提供超过10年ETF历史分钟数据权限：正式权限请参阅 权限说明",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "ETF代码，e.g. 159001.SZ"
    },
    {
      "name": "freq",
      "type": "str",
      "required": "Y",
      "description": "分钟频度（1min/5min/15min/30min/60min）"
    },
    {
      "name": "start_date",
      "type": "datetime",
      "required": "N",
      "description": "开始日期 格式：2025-06-01 09:00:00"
    },
    {
      "name": "end_date",
      "type": "datetime",
      "required": "N",
      "description": "结束时间 格式：2025-06-20 19:00:00"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "ETF代码"
    },
    {
      "name": "trade_time",
      "type": "str",
      "required": "Y",
      "description": "交易时间"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘价"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘价"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高价"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低价"
    },
    {
      "name": "vol",
      "type": "int",
      "required": "Y",
      "description": "成交量"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交金额"
    }
  ]
}