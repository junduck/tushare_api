{
  "api_name": "hk_hold",
  "title": "沪深股通持股明细",
  "description": "获取沪深港股通持股明细，数据来源港交所。限量：单次最多提取3800条记录，可循环调取，总量不限制积分：用户积120积分可调取试用，2000积分可正常使用，单位分钟有流控，积分越高流量越大，请自行提高积分，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "code",
      "type": "str",
      "required": "N",
      "description": "交易所代码"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS股票代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "类型：SH沪股通（北向）SZ深股通（北向）HK港股通（南向持股）"
    }
  ],
  "output_params": [
    {
      "name": "code",
      "type": "str",
      "required": "Y",
