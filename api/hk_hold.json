      "type": "str",
      "required": "Y",
      "description": "TS代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "vol",
      "type": "int",
      "required": "Y",
      "description": "持股数量(股)"
    },
    {
      "name": "ratio",
      "type": "float",
      "required": "Y",
      "description": "持股占比（%），占已发行股份百分比"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "Y",
      "description": "类型：SH沪股通SZ深股通HK港股通"
    }
