{
  "api_name": "us_tradecal",
  "title": "美股交易日历",
  "description": "获取美股交易日历信息限量：单次最大6000，可根据日期阶段获取",
  "input_params": [
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "is_open",
      "type": "str",
      "required": "N",
      "description": "是否交易"
    }
  ],
  "output_params": [
    {
      "name": "cal_date",
      "type": "str",
      "required": "Y",
      "description": "日历日期"
    },
    {
      "name": "is_open",
      "type": "int",
      "required": "Y",
      "description": "是否交易 '0'休市 '1'交易"
    },
    {
      "name": "pretrade_date",
      "type": "str",
      "required": "Y",
      "description": "上一个交易日"
    }
  ]
}