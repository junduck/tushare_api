{
  "api_name": "fx_daily",
  "title": "外汇日线行情",
  "description": "获取外汇日线行情限量：单次最大提取1000行记录，可多次提取，总量不限制积分：用户需要至少2000积分才可以调取，但有流量控制，5000积分以上频次相对较高，积分越多权限越大，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（GMT，日期是格林尼治时间，比北京时间晚一天）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期（GMT）"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期（GMT）"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易商，目前只有FXCM"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "外汇代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "bid_open",
      "type": "float",
      "required": "Y",
      "description": "买入开盘价"
    },
    {
      "name": "bid_close",
      "type": "float",
      "required": "Y",
      "description": "买入收盘价"
    },
    {
      "name": "bid_high",
      "type": "float",
      "required": "Y",
      "description": "买入最高价"
    },
    {
      "name": "bid_low",
      "type": "float",
      "required": "Y",
      "description": "买入最低价"
    },
    {
      "name": "ask_open",
      "type": "float",
      "required": "Y",
      "description": "卖出开盘价"
    },
    {
      "name": "ask_close",
      "type": "float",
      "required": "Y",
      "description": "卖出收盘价"
    },
    {
      "name": "ask_high",
      "type": "float",
      "required": "Y",
      "description": "卖出最高价"
    },
    {
      "name": "ask_low",
      "type": "float",
      "required": "Y",
      "description": "卖出最低价"
    },
    {
      "name": "tick_qty",
      "type": "int",
      "required": "Y",
      "description": "报价笔数"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易商"
    }
  ]
}