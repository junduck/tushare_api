{
  "api_name": "trade_cal",
  "title": "交易日历",
  "description": "获取各大期货交易所交易日历数据积分：需2000积分才可以提取数据",
  "input_params": [
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易所 SHFE 上期所 DCE 大商所 CFFEX中金所  CZCE郑商所 INE上海国际能源交易所"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "is_open",
      "type": "int",
      "required": "N",
      "description": "是否交易 0休市 1交易"
    }
  ],
  "output_params": [
    {
      "name": "exchange",
      "type": "str",
      "required": "Y",
      "description": "交易所 同参数部分描述"
    },
    {
      "name": "cal_date",
      "type": "str",
      "required": "Y",
      "description": "日历日期"
    },
    {
      "name": "is_open",
      "type": "int",
      "required": "Y",
      "description": "是否交易 0休市 1交易"
    },
    {
      "name": "pretrade_date",
      "type": "str",
      "required": "N",
      "description": "上一个交易日"
    }
  ]
}