      "type": "str",
      "required": "N",
      "description": "交易日期 (二选一)"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期 (二选一)"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
