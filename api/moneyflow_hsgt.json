    {
      "name": "ggt_sz",
      "type": "float",
      "required": "港股通（深圳）",
      "description": ""
    },
    {
      "name": "hgt",
      "type": "float",
      "required": "沪股通（百万元）",
      "description": ""
    },
    {
      "name": "sgt",
      "type": "float",
      "required": "深股通（百万元）",
      "description": ""
    },
    {
      "name": "north_money",
      "type": "float",
      "required": "北向资金（百万元）",
      "description": ""
    },
    {
      "name": "south_money",
      "type": "float",
      "required": "南向资金（百万元）",
      "description": ""
    }
  ]
}