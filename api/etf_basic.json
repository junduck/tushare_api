{
  "api_name": "etf_basic",
  "title": "ETF基本信息",
  "description": "获取国内ETF基础信息，包括了QDII。数据来源与沪深交易所公开披露信息。限量：单次请求最大放回5000条数据（当前ETF总数未超过2000）权限：用户积8000积分可调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "ETF代码（带.SZ/.SH后缀的6位数字，如：159526.SZ）"
    },
    {
      "name": "index_code",
      "type": "str",
      "required": "N",
      "description": "跟踪指数代码"
    },
    {
      "name": "list_date",
      "type": "str",
      "required": "N",
      "description": "上市日期（格式：YYYYMMDD）"
    },
    {
      "name": "list_status",
      "type": "str",
      "required": "N",
      "description": "上市状态（L上市 D退市 P待上市）"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "N",
      "description": "交易所（SH上交所 SZ深交所）"
    },
    {
      "name": "mgr",
      "type": "str",
      "required": "N",
      "description": "管理人（简称，e.g.华夏基金)"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "基金交易代码"
    },
    {
      "name": "csname",
      "type": "str",
      "required": "Y",
      "description": "ETF中文简称"
    },
    {
      "name": "extname",
      "type": "str",
      "required": "Y",
      "description": "ETF扩位简称(对应交易所简称)"
    },
    {
      "name": "cname",
      "type": "str",
      "required": "Y",
      "description": "基金中文全称"
    },
    {
      "name": "index_code",
      "type": "str",
      "required": "Y",
      "description": "ETF基准指数代码"
    },
    {
      "name": "index_name",
      "type": "str",
      "required": "Y",
      "description": "ETF基准指数中文全称"
    },
    {
      "name": "setup_date",
      "type": "str",
      "required": "Y",
      "description": "设立日期（格式：YYYYMMDD）"
    },
    {
      "name": "list_date",
      "type": "str",
      "required": "Y",
      "description": "上市日期（格式：YYYYMMDD）"
    },
    {
      "name": "list_status",
      "type": "str",
      "required": "Y",
      "description": "存续状态（L上市 D退市 P待上市）"
    },
    {
      "name": "exchange",
      "type": "str",
      "required": "Y",
      "description": "交易所（上交所SH 深交所SZ）"
    },
    {
      "name": "mgr_name",
      "type": "str",
      "required": "Y",
      "description": "基金管理人简称"
    },
    {
      "name": "custod_name",
      "type": "str",
      "required": "Y",
      "description": "基金托管人名称"
    },
    {
      "name": "mgt_fee",
      "type": "float",
      "required": "Y",
      "description": "基金管理人收取的费用"
    },
    {
      "name": "etf_type",
      "type": "str",
      "required": "Y",
      "description": "基金投资通道类型（境内、QDII）"
    }
  ]
}