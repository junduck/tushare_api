{
  "api_name": "rt_etf_k",
  "title": "ETF实时日线",
  "description": "获取ETF实时日k线行情，支持按ETF代码或代码通配符一次性提取全部ETF实时日k线行情限量：单次最大可提取2000条数据积分：本接口是单独开权限的数据，单独申请权限请参考权限列表",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "支持通配符方式，e.g. 5*.SH、15*.SZ、159101.SZ"
    },
    {
      "name": "topic",
      "type": "str",
      "required": "Y",
      "description": "分类参数，取上海ETF时，需要输入'HQ_FND_TICK'，参考下面例子"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "ETF代码"
    },
    {
      "name": "name",
      "type": "None",
      "required": "Y",
      "description": "ETF名称"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "昨收价"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高价"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘价"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低价"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘价（最新价）"
    },
    {
      "name": "vol",
      "type": "int",
      "required": "Y",
      "description": "成交量（股）"
    },
    {
      "name": "amount",
      "type": "int",
      "required": "Y",
      "description": "成交金额（元）"
    },
    {
      "name": "num",
      "type": "int",
      "required": "Y",
      "description": "开盘以来成交笔数"
    },
    {
      "name": "ask_volume1",
      "type": "int",
      "required": "N",
      "description": "委托卖盘（股）"
    },
    {
      "name": "bid_volume1",
      "type": "int",
      "required": "N",
      "description": "委托买盘（股）"
    },
    {
      "name": "trade_time",
      "type": "str",
      "required": "N",
      "description": "交易时间"
    }
  ]
}