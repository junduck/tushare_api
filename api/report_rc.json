{
  "api_name": "report_rc",
  "title": "券商盈利预测数据",
  "description": "获取券商（卖方）每天研报的盈利预测数据，数据从2010年开始，每晚19~22点更新当日数据限量：单次最大3000条，可分页和循环提取所有数据权限：本接口120积分可以试用，每天10次请求，正式权限需8000积分，每天可请求100000次，10000积分以上无总量限制。",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "股票代码"
    },
    {
      "name": "report_date",
      "type": "str",
      "required": "N",
      "description": "报告日期"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "报告开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "报告结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "report_date",
      "type": "str",
      "required": "Y",
      "description": "研报日期"
    },
    {
      "name": "report_title",
      "type": "str",
      "required": "Y",
      "description": "报告标题"
    },
    {
      "name": "report_type",
      "type": "str",
      "required": "Y",
      "description": "报告类型"
    },
    {
      "name": "classify",
      "type": "str",
      "required": "Y",
      "description": "报告分类"
    },
    {
      "name": "org_name",
      "type": "str",
      "required": "Y",
      "description": "机构名称"
    },
    {
      "name": "author_name",
      "type": "str",
      "required": "Y",
