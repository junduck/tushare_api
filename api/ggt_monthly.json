{
  "api_name": "ggt_monthly",
  "title": "港股通每月成交统计",
  "description": "港股通每月成交信息，数据从2014年开始限量：单次最大1000积分：用户积5000积分可调取，请自行提高积分，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "month",
      "type": "str",
      "required": "N",
      "description": "月度（格式YYYYMM，下同，支持多个输入）"
    },
    {
      "name": "start_month",
      "type": "str",
      "required": "N",
      "description": "开始月度"
    },
    {
      "name": "end_month",
      "type": "str",
      "required": "N",
      "description": "结束月度"
    }
  ],
  "output_params": [
    {
      "name": "month",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "day_buy_amt",
      "type": "float",
      "required": "Y",
      "description": "当月日均买入成交金额（亿元）"
    },
    {
      "name": "day_buy_vol",
      "type": "float",
      "required": "Y",
      "description": "当月日均买入成交笔数（万笔）"
    },
    {
      "name": "day_sell_amt",
      "type": "float",
      "required": "Y",
      "description": "当月日均卖出成交金额（亿元）"
    },
    {
      "name": "day_sell_vol",
      "type": "float",
      "required": "Y",
      "description": "当月日均卖出成交笔数（万笔）"
    },
    {
      "name": "total_buy_amt",
      "type": "float",
      "required": "Y",
      "description": "总买入成交金额（亿元）"
    },
    {
      "name": "total_buy_vol",
      "type": "float",
      "required": "Y",
      "description": "总买入成交笔数（万笔）"
    },
    {
      "name": "total_sell_amt",
      "type": "float",
      "required": "Y",
      "description": "总卖出成交金额（亿元）"
    },
    {
      "name": "total_sell_vol",
      "type": "float",
      "required": "Y",
      "description": "总卖出成交笔数（万笔）"
    }
  ]
}