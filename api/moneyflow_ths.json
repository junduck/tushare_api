{
  "api_name": "moneyflow_ths",
  "title": "个股资金流向（THS）",
  "description": "获取同花顺个股资金流向数据，每日盘后更新限量：单次最大6000，可根据日期或股票代码循环提取数据积分：用户需要至少5000积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "股票代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式，下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "pct_change",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅"
    },
    {
      "name": "latest",
      "type": "float",
      "required": "Y",
      "description": "最新价"
    },
    {
      "name": "net_amount",
      "type": "float",
      "required": "Y",
      "description": "资金净流入(万元)"
    },
    {
      "name": "net_d5_amount",
      "type": "float",
      "required": "Y",
      "description": "5日主力净额(万元)"
    },
    {
      "name": "buy_lg_amount",
      "type": "float",
      "required": "Y",
      "description": "今日大单净流入额(万元)"
    },
    {
      "name": "buy_lg_amount_rate",
      "type": "float",
      "required": "Y",
      "description": "今日大单净流入占比(%)"
    },
    {
      "name": "buy_md_amount",
      "type": "float",
      "required": "Y",
      "description": "今日中单净流入额(万元)"
    },
    {
      "name": "buy_md_amount_rate",
      "type": "float",
      "required": "Y",
      "description": "今日中单净流入占比(%)"
    },
    {
      "name": "buy_sm_amount",
      "type": "float",
      "required": "Y",
      "description": "今日小单净流入额(万元)"
    },
    {
      "name": "buy_sm_amount_rate",
      "type": "float",
      "required": "Y",
      "description": "今日小单净流入占比(%)"
    }
  ]
}