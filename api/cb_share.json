{
  "api_name": "cb_share",
  "title": "可转债转股结果",
  "description": "获取可转债转股结果限量：单次最大2000，总量不限制权限：用户需要至少2000积分才可以调取，但有流量控制，5000积分以上频次相对较高，积分越多权限越大，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "转债代码，支持多值输入"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "Y",
      "description": "公告日期（YYYYMMDD格式，下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "公告开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "公告结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "债券代码"
    },
    {
      "name": "bond_short_name",
      "type": "str",
      "required": "Y",
      "description": "债券简称"
    },
    {
      "name": "publish_date",
      "type": "str",
      "required": "Y",
      "description": "公告日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "Y",
      "description": "统计截止日期"
    },
    {
      "name": "issue_size",
      "type": "float",
      "required": "Y",
      "description": "可转债发行总额"
    },
    {
      "name": "convert_price_initial",
      "type": "float",
      "required": "Y",
      "description": "初始转换价格"
    },
    {
      "name": "convert_price",
      "type": "float",
      "required": "Y",
      "description": "本次转换价格"
    },
    {
      "name": "convert_val",
      "type": "float",
      "required": "Y",
      "description": "本次转股金额"
    },
    {
      "name": "convert_vol",
      "type": "float",
      "required": "Y",
      "description": "本次转股数量"
    },
    {
      "name": "convert_ratio",
      "type": "float",
      "required": "Y",
      "description": "本次转股比例"
    },
    {
      "name": "acc_convert_val",
      "type": "float",
      "required": "Y",
      "description": "累计转股金额"
    },
    {
      "name": "acc_convert_vol",
      "type": "float",
      "required": "Y",
      "description": "累计转股数量"
    },
    {
      "name": "acc_convert_ratio",
      "type": "float",
      "required": "Y",
      "description": "累计转股比例"
    },
    {
      "name": "remain_size",
      "type": "float",
      "required": "Y",
      "description": "可转债剩余金额"
    },
    {
      "name": "total_shares",
      "type": "float",
      "required": "Y",
      "description": "转股后总股本"
    }
  ]
}