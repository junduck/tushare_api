    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "基金代码，支持多只基金，逗号分隔"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "N",
      "description": "公告日期，格式：YYYYMMDD"
    },
    {
      "name": "name",
      "type": "str",
      "required": "N",
      "description": "基金经理姓名"
    },
    {
      "name": "offset",
      "type": "intint",
      "required": "N",
      "description": "开始行数"
    },
    {
      "name": "limit",
      "type": "int",
      "required": "N",
      "description": "每页行数"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "基金代码"
    },
    {
      "name": "ann_date",
      "type": "str",
      "required": "Y",
      "description": "公告日期"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "基金经理姓名"
    },
    {
      "name": "gender",
      "type": "str",
      "required": "Y",
      "description": "性别"
    },
    {
      "name": "birth_year",
      "type": "str",
      "required": "Y",
      "description": "出生年份"
    },
    {
      "name": "edu",
      "type": "str",
      "required": "Y",
      "description": "学历"
    },
    {
      "name": "nationality",
      "type": "str",
      "required": "Y",
      "description": "国籍"
    },
    {
      "name": "begin_date",
      "type": "str",
      "required": "Y",
      "description": "任职日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "Y",
      "description": "离任日期"
