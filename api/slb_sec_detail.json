  "input_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期（YYYYMMDD格式，下同）"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "股票代码"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期（YYYYMMDD）"
    },
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "name",
      "type": "str",
      "required": "Y",
      "description": "股票名称"
    },
    {
      "name": "tenor",
      "type": "str",
      "required": "Y",
      "description": "期 限(天)"
    },
    {
      "name": "fee_rate",
      "type": "float",
      "required": "Y",
      "description": "融出费率(%)"
    },
    {
      "name": "lent_qnt",
      "type": "float",
      "required": "Y",
      "description": "转融券融出数量(万股)"
    }
  ]
}