    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交金额(万元)"
    },
    {
      "name": "oi",
      "type": "float",
      "required": "Y",
      "description": "持仓量(手)"
    }
  ]
}