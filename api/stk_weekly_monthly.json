{
  "api_name": "stk_weekly_monthly",
  "title": "周/月线行情(每日更新)",
  "description": "股票周/月线行情(每日更新)限量：单次最大6000,可使用交易日期循环提取，总量不限制积分：用户需要至少2000积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "TS代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期(YYYYMMDD，下同)"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始交易日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束交易日期"
    },
    {
      "name": "freq",
      "type": "str",
      "required": "Y",
      "description": "频率week周，month月"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期（每周五或者月末日期）"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "Y",
      "description": "计算截至日期"
    },
    {
      "name": "freq",
      "type": "str",
      "required": "Y",
      "description": "频率(周week,月month)"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "(周/月)开盘价"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "(周/月)最高价"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "(周/月)最低价"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "(周/月)收盘价"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "上一(周/月)收盘价"
    },
    {
      "name": "vol",
      "type": "float",
      "required": "Y",
      "description": "(周/月)成交量"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "(周/月)成交额"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "(周/月)涨跌额"
    },
    {
      "name": "pct_chg",
      "type": "float",
      "required": "Y",
      "description": "(周/月)涨跌幅(未复权,如果是复权请用 通用行情接口)"
    }
  ]
}