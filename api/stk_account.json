{
  "api_name": "stk_account",
  "title": "股票开户数据（停）",
  "description": "获取股票账户开户数据，统计周期为一周积分：600积分可调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "date",
      "type": "str",
      "required": "N",
      "description": "日期"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "date",
      "type": "str",
      "required": "Y",
      "description": "统计周期"
    },
    {
      "name": "weekly_new",
      "type": "float",
      "required": "Y",
      "description": "本周新增（万）"
    },
    {
      "name": "total",
      "type": "float",
      "required": "Y",
      "description": "期末总账户数（万）"
    },
    {
      "name": "weekly_hold",
      "type": "float",
      "required": "Y",
      "description": "本周持仓账户数（万）"
    },
    {
      "name": "weekly_trade",
      "type": "float",
      "required": "Y",
      "description": "本周参与交易账户数（万）"
    }
  ]
}