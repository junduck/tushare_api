{
  "api_name": "dc_daily",
  "title": "东财概念和行业指数行情",
  "description": "获取东财概念板块、行业指数板块、地域板块行情数据，历史数据开始于2020年限量：单次最大2000条数据，可根据日期参数循环获取权限：用户积累6000积分可调取，具体请参阅积分获取办法注意：本接口只限个人学习和研究使用，如需商业用途，请自行联系东方财富解决数据采购问题。",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "板块代码（格式：xxxxx.DC)"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期(格式：YYYYMMDD下同）"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    },
    {
      "name": "idx_type",
      "type": "str",
      "required": "N",
      "description": "板块类型： 概念板块、行业板块、地域板块"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "板块代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘点位"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘点位"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高点位"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低点位"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "涨跌点位"
    },
    {
      "name": "pct_change",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅"
    },
    {
      "name": "vol",
      "type": "float",
      "required": "Y",
      "description": "成交量"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交额"
    },
    {
      "name": "swing",
      "type": "float",
      "required": "Y",
      "description": "振幅"
    },
    {
      "name": "turnover_rate",
      "type": "float",
      "required": "Y",
      "description": "换手率"
    }
  ]
}