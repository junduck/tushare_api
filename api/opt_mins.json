{
  "api_name": "opt_mins",
  "title": "期权分钟行情",
  "description": "获取全市场期权合约分钟数据，支持1min/5min/15min/30min/60min行情，提供Python SDK和 http Restful API两种方式。限量：单次最大8000行数据，可以通过合约代码和时间循环获取。权限：120积分可以调取2次接口查看数据，正式权限请参阅 权限说明  。",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码，e.g：10007976.SH"
    },
    {
      "name": "freq",
      "type": "str",
      "required": "Y",
      "description": "分钟频度（1min/5min/15min/30min/60min）"
    },
    {
      "name": "start_date",
      "type": "datetime",
      "required": "N",
      "description": "开始日期 格式：2024-08-25 09:00:00"
    },
    {
      "name": "end_date",
      "type": "datetime",
      "required": "N",
      "description": "结束时间 格式：2024-08-25 19:00:00"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "股票代码"
    },
    {
      "name": "trade_time",
      "type": "str",
      "required": "Y",
      "description": "交易时间"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘价"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘价"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高价"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低价"
    },
    {
      "name": "vol",
      "type": "int",
      "required": "Y",
      "description": "成交量"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交金额"
    },
    {
      "name": "oi",
      "type": "float",
      "required": "Y",
      "description": "持仓量"
    }
  ]
}