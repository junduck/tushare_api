{
  "api_name": "sge_daily",
  "title": "上海黄金现货日行情",
  "description": "获取上海黄金交易所现货合约日线行情限量：单次最大2000，可循环或者分页提取积分：用户积2000积分可调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "合约代码，可通过基础信息获得"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "现货合约代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘点(元/克)"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘点(元/克)"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高点(元/克)"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低点(元/克)"
    },
    {
      "name": "price_avg",
      "type": "float",
      "required": "Y",
      "description": "加权平均价(元/克)"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "涨跌点位(元/克)"
    },
    {
      "name": "pct_change",
      "type": "float",
