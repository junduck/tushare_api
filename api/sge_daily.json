    },
    {
      "name": "oi",
      "type": "float",
      "required": "Y",
      "description": "市场持仓"
    },
    {
      "name": "settle_vol",
      "type": "float",
      "required": "Y",
      "description": "交收量"
    },
    {
      "name": "settle_dire",
      "type": "str",
      "required": "Y",
      "description": "持仓方向"
    }
  ]
