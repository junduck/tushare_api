{
  "api_name": "fund_daily",
  "title": "ETF日线行情",
  "description": "获取ETF行情每日收盘后成交数据，历史超过10年限量：单次最大2000行记录，可以根据ETF代码和日期循环获取历史，总量不限制积分：需要至少2000积分才可以调取，具体请参阅积分获取办法",
  "input_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "N",
      "description": "基金代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "N",
      "description": "交易日期(YYYYMMDD格式，下同)"
    },
    {
      "name": "start_date",
      "type": "str",
      "required": "N",
      "description": "开始日期"
    },
    {
      "name": "end_date",
      "type": "str",
      "required": "N",
      "description": "结束日期"
    }
  ],
  "output_params": [
    {
      "name": "ts_code",
      "type": "str",
      "required": "Y",
      "description": "TS代码"
    },
    {
      "name": "trade_date",
      "type": "str",
      "required": "Y",
      "description": "交易日期"
    },
    {
      "name": "open",
      "type": "float",
      "required": "Y",
      "description": "开盘价(元)"
    },
    {
      "name": "high",
      "type": "float",
      "required": "Y",
      "description": "最高价(元)"
    },
    {
      "name": "low",
      "type": "float",
      "required": "Y",
      "description": "最低价(元)"
    },
    {
      "name": "close",
      "type": "float",
      "required": "Y",
      "description": "收盘价(元)"
    },
    {
      "name": "pre_close",
      "type": "float",
      "required": "Y",
      "description": "昨收盘价(元)"
    },
    {
      "name": "change",
      "type": "float",
      "required": "Y",
      "description": "涨跌额(元)"
    },
    {
      "name": "pct_chg",
      "type": "float",
      "required": "Y",
      "description": "涨跌幅(%)"
    },
    {
      "name": "vol",
      "type": "float",
      "required": "Y",
      "description": "成交量(手)"
    },
    {
      "name": "amount",
      "type": "float",
      "required": "Y",
      "description": "成交额(千元)"
    }
  ]
}